        "def checkAccess(user, file, action):\n",
        "    roles = getUserRoles(user)\n",
        "    permissions = getInheritedPermissions(file)\n",
        "    for role in roles:\n",
        "        # Asumsi: 'check_inherited_permission' adalah fungsi yang mengembalikan boolean,\n",
        "        # atau bagian 'inherited' pada aslinya dimaksudkan sebagai variabel boolean.\n",
        "        # Perlu konteks lebih lanjut untuk interpretasi yang tepat dari 'inherited'.\n",
        "        if (file, role, action) in ACLTable or check_inherited_permission(parent(file), role, action):\n",
        "            logAccess(user, file, action, \"granted\")\n",
        "            return \"Access Granted\"\n",
        "    logAccess(user, file, action, \"denied\")\n",
        "    return \"Access Denied\""
      ]
    }
  ]
